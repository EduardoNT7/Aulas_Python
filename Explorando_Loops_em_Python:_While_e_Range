contador = 10

while contador >=1:
    print('Contador - ', contador)
    contador -= 1
    if contador == 0:
      print('fogo')

par = int(input('insira um numero inteiro positivo '))
soma = sum((range(2,par,2)))
print(soma)

num = int(input('Insira um n√∫mero int'))
i = 1

while i <= 10:
    resultado = num * i
    print(f"{num} x {i} = {resultado}")
    i += 1

numero = 99
while numero >= 1:
    print(numero)
    numero -= 1
