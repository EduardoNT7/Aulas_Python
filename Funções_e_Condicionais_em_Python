def sub (a, b):
    print("O valor da subtração é: ", a-b)

sub(5,3)

#2

def sum (a, b):
    print("O valor da subtração é: ", a+b)

sum(5,3)

#3
def div (a, b):
    print("O valor da subtração é: ", a/b)

div(4,2)

#4

def fun():
    print('Você deseja somar, subtrair, dividir ou multiplicar? ')
    escolha1 = int(input("Digte 1 para somar, Digite 2 para subtrair, Digite 3 para dividir, Digite 4 para multiplicar "))
    if escolha1 == 1:
      somar()

    elif escolha1 == 2:
      subtrair()

    elif escolha1 == 3:
      dividir()

    elif escolha1 == 4:
      multiplicar()
    else:
      print('Opção inválida')

def somar():
    n1 = int(input('Digite um numero : '))
    n2 = int(input('Digite um numero : '))
    resultado = n1+n2
    print('Resultado é = ', resultado)

def subtrair():
    n1 = int(input('Digite um numero : '))
    n2 = int(input('Digite um numero : '))
    resultado = n1-n2
    print('Resultado é = ', resultado)

def dividir():
    n1 = int(input('Digite um numero : '))
    n2 = int(input('Digite um numero : '))
    if n2 == 0:
      print('impossivel dividir por 0')
    else:
      resultado = n1/n2
      print('Resultado é = ', resultado)

def multiplicar():
    n1 = int(input('Digite um numero : '))
    n2 = int(input('Digite um numero : '))
    resultado = n1*n2
    print('Resultado é = ', resultado)

fun()

